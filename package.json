{
    "name": "fashion-scanner",
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "server": "tsx watch server/index.ts",
        "dev:full": "concurrently \"npm run dev\" \"npm run server\"",
        "cap:sync": "npm run build && npx cap sync",
