        "@capacitor/geolocation": "^6.1.1",
        "@capacitor/ios": "^6.2.1",
        "@capacitor/splash-screen": "^6.0.4",
        "@capacitor/status-bar": "^6.0.3",
        "@supabase/supabase-js": "^2.47.10",
        "@tanstack/react-query": "^5.62.7",
        "axios": "^1.7.9",
        "framer-motion": "^11.11.17",
        "lucide-react": "^0.468.0",
        "mapbox-gl": "^3.8.0",
        "react": "^18.3.1",
